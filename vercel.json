{
  "version": 2,
  "functions": {
    "app.py": {
      "runtime": "python3.9",
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
